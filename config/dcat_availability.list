<list xmlns="https://github.com/Region-Skane-SDI/diml/list" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://github.com/Region-Skane-SDI/diml/list https://raw.githubusercontent.com/Region-Skane-SDI/diml-schemas/main/v0/list.xsd" configurationUrl="https://raw.githubusercontent.com/Region-Skane-SDI/diml-samples/main/config/dcat_availability.list">
    <listItems>
        <listItem id="experimental">
            <names>
                <name language="sv">Experimentell</name>
            </names>
            <descriptions>
                <description type="plain" language="sv">Data är tillgänglig på försök för en kort
					period, kortsiktig planering.</description>
            </descriptions>
        </listItem>
        <listItem id="stable">
            <names>
                <name language="sv">Stabil</name>
            </names>
            <descriptions>
                <description type="plain" language="sv">Data kommer att vara tillgängliga på lång
					sikt.</description>
            </descriptions>
        </listItem>
        <listItem id="temporary">
            <names>
                <name language="sv">Temporär</name>
            </names>
            <descriptions>
                <description type="plain" language="sv">Data kan försvinna när som helst, ingen
					planering.</description>
            </descriptions>
        </listItem>
        <listItem id="available">
            <names>
                <name language="sv">Tillgänglig</name>
            </names>
            <descriptions>
                <description type="plain" language="sv">Data finns tillgänglig för några år,
					planering på medellång sikt.</description>
            </descriptions>
        </listItem>
    </listItems>
</list>